@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
}

.icon-right::before {
    content: "\e7eb";
}

.icon-left::before {
    content: "\e7ec";
}

.icon-doubleleft::before {
    content: "\e7ed";
}

.icon-doubleright::before {
    content: "\e7ee";
}
